{
  "pipeline": {
    "name": "allflights"
  },
  "transform": {
    "image": "nickharvey/pachyflights",
    "cmd": [ "/bin/bash" ],
    "stdin": [
  "python3 flights.py --output_dir /pfs/out --input_dir /pfs/"
    ]
  },
  "egress": {
    "URL": "gs://2017_2018_flights/output/"
  },
  "parallelism_spec": {
    "constant" : 1
  },
  "input": {
    "union" : [
      {
        "atom": {
          "repo": "2017flights",
          "glob": "/*"
        }
      },
      {
        "atom": {
          "repo": "2018flights",
          "glob": "/02/*"
        }
      }
    ] 
  }
}